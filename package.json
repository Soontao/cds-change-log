{
  "name": "cds-change-log",
  "version": "0.0.7",
  "main": "lib/index.js",
  "cds.main": "./index.cds",
  "description": "best practice of Change Log for CAP nodejs runtime",
  "engines": {
    "node": ">=12",
    "npm": ">=6"
  },
  "author": {
    "name": "Theo Sun",
    "email": "theo.sun@outlook.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Soontao/cds-change-log"
  },
  "scripts": {
    "pretest": "bash ./install_no_trace_deps.sh",
    "prelint": "bash ./install_no_trace_deps.sh",
    "test": "jest --detectOpenHandles --passWithNoTests",
    "prebuild": "rimraf lib",
    "build": "tsc",
    "postbuild": "tsc --noEmit --p tsconfig.lib.json",
    "lint": "eslint src/** --fix",
    "release": "npx standard-version --no-verify",
    "coverage": "npm test -- --coverage --no-cache"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "17.0.3",
    "@types/jest": "28.1.6",
    "@types/node": "16.11.47",
    "@typescript-eslint/eslint-plugin": "5.32.0",
    "@typescript-eslint/parser": "5.32.0",
    "commitlint": "17.0.3",
    "eslint": "^8.17.0",
    "husky": "~4.3.8",
    "jest": "28.1.3",
    "rimraf": "^3.0.2",
    "ts-jest": "28.0.7",
    "ts-node": "10.9.1",
    "typescript": "^4.7.3"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "overrides": {
    "@sap/xsenv": "3.2.0"
  },
  "license": "Apache-2.0",
  "keywords": [
    "SAP",
    "CAP",
    "Change Log",
    "Framework"
  ]
}
